{
  "manifest_version": 2,
  "name": "colorful-again",
  "version": "1.0.0",
  "description": "Remove global gray filter.",
  "icons": {
    "16": "icons/icon_16x16.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["colorful-again.js"]
    }
  ]
}
